<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pedromoreirareisgmail.geoquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".activitys.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:scheme="https"
                    android:host="www.facebook.com"
                    android:pathPrefix="/photos/?ref=page_internal" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activitys.TypeActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.PlayActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.DoneActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activitys.DetailActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>